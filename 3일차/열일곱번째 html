<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <script>
        let dan = prompt("단을 입력해주세요");
        let result;

        document.write("<h1>" + dan + "단</h1>")
        for (let i=1; i<=9; i++){
            result = i * dan;
            document.write(dan +" X "+i+ " = " +result+ "<br>")
        }
    </script>
</head>
<body>
    
</body>
</html>
